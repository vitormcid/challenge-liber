<%= render "style" %>

<%stylesheet_link_tag("login")%>
<div class="login-page">

  <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
    <div class="form">      
       <%= image_tag("banner.png", size: "260x80", :style => 'margin-bottom: 10px', alt: "Ruby on Rails")%>
      <form class="login-form">    
        <%= f.text_field :email, autofocus: true,value: "email", class: "form-control" %>    
        <%= f.password_field :password, autocomplete: "off", value:"xxxxxxxxxxxx", class: "form-control" %>
        <%= f.submit "Log in", class: "btn btn-primary"%>
       
        <%= render "devise/shared/links" %>
      </form>
    </div>
  <% end %>  
</div>